diff --git a/code/tools/stringify.c b/code/tools/stringify.c
new file mode 100644
index 00000000..a5ce84b0
--- /dev/null
+++ b/code/tools/stringify.c
@@ -0,0 +1,51 @@
+#include <stdio.h>
+#include <ctype.h>
+#include <string.h>
+#include <libgen.h>
+
+int main(int argc, char **argv)
+{
+    FILE *ifp;
+    FILE *ofp;
+    char buffer[1024];
+
+    if(argc < 3)
+        return 1;
+
+    char *inFile = argv[1];
+    char *outFile = argv[2];
+
+    ifp = fopen(inFile, "r");
+    if(!ifp)
+        return 2;
+
+    ofp = fopen(outFile, "w");
+    if(!ofp)
+        return 3;
+
+    // Strip extension
+    char *base = basename(inFile);
+    *strrchr(base, '.') = '\0';
+
+    fprintf(ofp, "const char *fallbackShader_%s =\n", base);
+
+    while(fgets(buffer, sizeof(buffer), ifp))
+    {
+        // Strip trailing whitespace from line
+        char *end = buffer + strlen(buffer) - 1;
+        while(end >= buffer && isspace(*end))
+            end--;
+
+        end[1] = '\0';
+
+        // Write line enquoted, with a newline
+        fprintf(ofp, "\"%s\\n\"\n", buffer);
+    }
+
+    fprintf(ofp, ";\n");
+
+    fclose(ifp);
+    fclose(ofp);
+
+    return 0;
+}
